{
  "name": "workscope-data-service",
  "version": "1.0.0",
  "main": "./src/app.js",
  "scripts": {
    "clean": "rm -r dist/*",
    "fix": "eslint --fix src test",
    "lint": "eslint ./src/* ./test/**/*.js",
    "test": "NODE_ENV=test PORT=0 istanbul cover --dir coverage/cobertura _mocha test test/utils",
    "start": "node app.js",
    "predevStart": "npm run lint",
    "devStart": "node ./src/app.js",
    "dev": "nodemon --exec npm run devStart",
    "dist": "cp -R src/. dist && cp -R migrations dist && cp package.json database.json dist/"
  },
  "dependencies": {
    "async": "^2.0.1",
    "body-parser": "~1.13.2",
    "bookshelf": "^0.10.1",
    "db-migrate": "^0.10.0-beta.16",
    "db-migrate-pg": "^0.1.10",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-bearer-token": "^2.1.0",
    "helmet": "^2.0.0",
    "knex": "^0.11.0",
    "lodash": "^4.13.1",
    "multer": "^1.2.0",
    "pg": "^6.1.0",
    "predix-fast-token": "^1.0.0",
    "xlsx": "^0.8.0"
  },
  "devDependencies": {
    "bluebird": "^3.4.0",
    "chai": "^3.5.0",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "istanbul": "^0.4.3",
    "karma": "^0.13.22",
    "mocha": "^2.4.5",
    "mock-knex": "^0.3.4",
    "nodemon": "^1.10.2",
    "proxyquire": "^1.7.9",
    "sinon": "^1.17.3",
    "supertest": "^1.2.0"
  }
}
